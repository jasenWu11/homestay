/* pages/room_list/room_list.wxss */
@import '/style/weui.wxss';
page{
  width: 100%;
  height: 100%;
}
.main_panel{
  height: 100%;
  display: flex;
  flex-direction: column;
  overflow: hidden;
}
.main_panel .top_panel{
  background-color: #ffffff;
  display: flex;
  flex-direction: column;
  padding: 10rpx 0rpx 0rpx 0rpx;
  box-sizing: border-box;
  height: 170rpx;
}

.top_panel .top_top_panel{
  display: flex;
  align-items: center;
  margin: 0rpx 40rpx;
}

.top_top_panel .search_panel{
  padding: 18rpx;
  box-sizing: border-box;
  border-width: 1rpx;
  border-style: solid;
  border-color: #333333;
  border-radius: 10rpx;
  display: flex;
  align-items: center;
  flex: 1;
}

.search_panel .search_icon{
  width: 34rpx;
  height: 34rpx;
}

.search_panel .input_panel{
  flex: 1;
  display: flex;
  align-items: center;
  margin-left: 15rpx;
}

.search_panel .time_panel{
  display: flex;
  color: #009966;
}

.top_top_panel .map_panel{
  margin-left: 20rpx;
  color: #000000;
  display: flex;
  align-items: center;
}

.map_panel .map_icon{
  width: 64rpx;
  height: 50rpx;
  margin-right: 10rpx;
}

.top_panel .screen_panel{
  display: flex;
  height: 60rpx;
  margin-top: 10rpx;
  align-items: center;
  justify-content: space-between;
}

.screen_panel .screen_item_panel{
  flex: 1;
  display: flex;
  color: #000000;
  font-size: 26rpx;
  justify-content: center;
  align-items: center;
}

.screen_item_panel .screen_item_icon{
  height: 30rpx;
  width: 30rpx;
}



.screen_main_panel{
  position:fixed;
  width: 100%;
  left: 0;
  top: 168rpx;
  bottom: 0rpx;
  z-index: 2;
  display: flex;
  flex-direction: column;
}

.screen_main_panel .screen_main_1{
  width: 100%;
  z-index: 3;
}

.screen_main_1 .screen_view_1{
  width: 100%;
  background-color: #ffffff;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 20rpx 40rpx;
  box-sizing: border-box;
}

.screen_view_1 .screen_main1_item{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 20%;
  border-width: 1rpx;
  border-style: solid;
  border-radius: 8rpx;
  margin-bottom: 25rpx;
  padding: 10rpx 0rpx;
  font-size: 26rpx;
}

.screen_main_panel .screen_main_2{
  width: 100%;
  z-index: 3;
}

.screen_main_2 .screen_view_2{
  width: 100%;
  background-color: #ffffff;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 20rpx 40rpx;
  box-sizing: border-box;
}

.screen_view_2 .price_panel{
  width: 100%;
  margin: 10rpx 0rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.price_panel .price_panel_item{
  min-width: 100rpx;
  background-color: #19896f;
  color: #ffffff;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  padding: 6rpx 0rpx;
  border-radius: 6rpx;
}

.screen_view_2 .zy-slider{
  width: 670rpx;
}

.screen_view_2 .reset_button{
  color: #19896f;
  border-width: 1rpx;
  border-style: solid;
  border-color: #19896f;
  border-radius: 6rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 26rpx;
  padding: 15rpx 30rpx;
}

.screen_view_2 .save_button{
  color: #ffffff;
  background-color: #19896f;
  border-radius: 6rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 26rpx;
  padding: 15rpx 30rpx;
  flex: 1;
  margin-left: 20rpx;
}

.screen_main_panel .screen_main_3{
  width: 100%;
  z-index: 3;
}

.screen_main_3 .screen_view_3{
  width: 100%;
  background-color: #ffffff;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 20rpx 40rpx;
  box-sizing: border-box;
}

.screen_view_3 .screen_main3_item{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 45%;
  border-width: 1rpx;
  border-style: solid;
  border-radius: 8rpx;
  margin-bottom: 25rpx;
  padding: 15rpx 0rpx;
  font-size: 26rpx;
}

.screen_main_panel .screen_main_4{
  width: 100%;
  z-index: 3;
}

.screen_main_4 .screen_view_4{
  width: 100%;
  background-color: #ffffff;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 20rpx 40rpx;
  box-sizing: border-box;
}

.screen_view_4 .screen_view_4_item{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 10rpx;
}

.screen_main_panel .cover_panel{
  flex: 1;
  width: 100%;
  background-color: #1312128a;
}

.cover_panel2{
  position:fixed;
  width: 100%;
  top: 0rpx;
  height: 98rpx;
  z-index: 2;
  background-color: #ffffffd5
}

.main_panel .content_panel{
  flex: 1;
  padding: 0rpx 40rpx;
  box-sizing: border-box;
  overflow: auto;
  background-color: #ffffff;
}

.line {
  width: 100%;
  height: 0.6rpx;
  background-color: #bbbbbb;
}

.house_list_view {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin-top: 10rpx;
}

.house_list_view .house_view {
  width: 100%;
  display: flex;
  margin-bottom: 25rpx;
  flex-direction: column;
  box-shadow: 2rpx 2rpx 10rpx 10rpx #eeeeee;
  border-radius: 15rpx;
}

.house_list_view .house_view .collect_image{
  width: 60rpx;
  height: 60rpx;
  z-index: 999;
  position:relative;
  margin-bottom: -80rpx;
  margin-left: 590rpx;
}

.slide-box .house_view {
  width: 304rpx;
  display: flex;
  margin-bottom: 0rpx;
  flex-direction: column;
  margin-top: 20rpx;
}

.slide-box .house_view .collect_image{
  width: 40rpx;
  height: 40rpx;
  z-index: 999;
  position:relative;
  margin-bottom: -50rpx;
  margin-left: 244rpx;
  box-sizing: border-box;
}

.house_view .house_name {
  font-size: 28rpx;
  font-family: SimSun;
  margin: 15rpx 0rpx 30rpx 0rpx;
  color: #19896f;
  padding: 0rpx 20rpx;
  box-sizing: border-box;
}

.house_view .house_image {
  width: 100%;
  border-radius: 5rpx;
}

.house_view .specification_text {
  font-size: 24rpx;
  font-family: SimSun;
  margin-top: 5rpx;
  padding: 20rpx;
}

.house_view .introduce_text {
  font-size: 28rpx;
  font-family: SimSun;
  margin: 5rpx 0rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
  padding: 20rpx;
  box-sizing: border-box;
}

.house_view .row_view {
  width: 100%;
  display: flex;
  flex-direction: row-reverse;
  align-items: center;
  padding: 20rpx;
  box-sizing: border-box;
}

.row_view .praise_view {
  font-size: 24rpx;
  font-family: SimSun;
}

.row_view .image_star {
  width: 24rpx;
  height: 24rpx;
}

.row_view .price {
  font-size: 26rpx;
  font-family: SimSun;
  flex: 1;
}